{
  "presets": ["@babel/preset-env", "@babel/react"],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "add-module-exports",
    ["babel-plugin-root-import", {
      "rootPathSuffix": "src",
      "rootPathPrefix": "@"
    }],
    "@babel/plugin-syntax-dynamic-import",
    "dynamic-import-node",
    ["babel-plugin-transform-builtin-extend", {
        "globals": ["Error"]
    }]
  ]
}
